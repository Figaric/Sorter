<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">

    <FontFamily x:Key="LatoThin">pack://application;,,,/Assets/Fonts/#Lato Hairline</FontFamily>
    <FontFamily x:Key="LatoRegular">pack://application;,,,/Assets/Fonts/#Lato</FontFamily>
    <FontFamily x:Key="LatoBold">pack://application;,,,/Assets/Fonts/#Lato Bold</FontFamily>
    <FontFamily x:Key="FontAwesome">pack://application;,,,/Assets/Fonts/#Font Awesome 5 Free Solid</FontFamily>

    <system:Double x:Key="FontSizeSmall">11</system:Double>
    <system:Double x:Key="FontSizeRegular">14</system:Double>
    <system:Double x:Key="FontSizeAverage">17</system:Double>
    <system:Double x:Key="FontSizeMedium">20</system:Double>
    <system:Double x:Key="FontSizeLarge">25</system:Double>
    <system:Double x:Key="FontSizeXLarge">30</system:Double>
    <system:Double x:Key="FontSizeXXLarge">40</system:Double>
    <system:Double x:Key="FontSizeXXXLarge">50</system:Double>

    <system:String x:Key="FontAwesomePlusIcon">&#xf067;</system:String>
    <system:String x:Key="FontAwesomeOpenedFolderIcon">&#xf07c;</system:String>
    
</ResourceDictionary>